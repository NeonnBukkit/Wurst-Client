---
layout: none
---
[
	{% for element in site.pages %}
	{
		"title": "{{ element.title | escape }}",
		"exerpt": "{{ element.exerpt | strip_html }}",
		"category": "{{ element.category }}",
		"tags": "{% for tag in element.tags %}tag{% unless forloop.last %}, {% endunless %}{% endfor %}",
		"url": "{{ site.baseurl }}{{ element.url }}",
		"date": "{{ element.date }}"
	} {% unless forloop.last %},{% endunless %}
	{% endfor %}
]