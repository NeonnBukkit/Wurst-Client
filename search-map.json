---
layout: none
---
[
	{% for element in site.pages %}{% comment %}
{% endcomment %}{% unless element.url == "/search-map.json" or element.url == "/404.html" %}{% comment %}
{% endcomment %}{
		"title": "{{ element.title | escape_once }}",
		"content": "{{ element.content | escape_once }}",
		"category": "{{ element.category }}",
		"tags": "{% for tag in element.tags %}tag{% unless forloop.last %}, {% endunless %}{% endfor %}",
		"url": "{{ site.baseurl }}{{ element.url }}",
		"date": "{{ element.date }}"
	} {% unless forloop.last %},{% endunless %}{% comment %}
{% endcomment %}{% endunless %}{% comment %}
{% endcomment %}{% endfor %}
]